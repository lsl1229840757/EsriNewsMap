<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping default-access="property" package="com.esri.entity">
		
	<class name="News" table="history_news">
		<id name="id" type="integer" column="ID">
			<generator class="native" />
		</id>
		<property name="origin" column="origin"/>
		<property name="keyWord" column="key_words"/>
		<property name="webUrl" column="web_url"/>
		<property name="mobileUrl" column="mobile_url"/>
		<property name="picUrl" column="pic_url"/>
		<property name="longitude" column="lng"/>
		<property name="latitude" column="lat"/>
		<property name="pubDate" column="time"/>
		<property name="title" column="title"/>
		
	</class>
</hibernate-mapping>
