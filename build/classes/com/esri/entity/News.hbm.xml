<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.esri.entity.News" table="recent_news">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="origin" type="java.lang.String">
            <column name="origin" />
        </property>
        <property name="keyWord" type="java.lang.String">
            <column name="key_words" />
        </property>
        <property name="webUrl" type="java.lang.String">
            <column name="web_url" />
        </property>
        <property name="mobileUrl" type="java.lang.String">
            <column name="mobile_url" />
        </property>
        <property name="picUrl" type="java.lang.String">
            <column name="pic_url" />
        </property>
        <property name="longitude" type="double">
            <column name="lng" />
        </property>
        <property name="latitude" type="double">
            <column name="lat" />
        </property>
        <property name="title" type="java.lang.String">
            <column name="title" />
        </property>
        <property name="pubDate" type="int">
            <column name="time" />
        </property>
    </class>
</hibernate-mapping>
