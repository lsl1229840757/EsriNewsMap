<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping default-access="property" package="com.esri.entity">
		
	<class name="News" table="news">
		<id name="id" type="integer" column="ID">
			<generator class="native" />
		</id>
		<property name="origin" column="ORIGIN"/>
		<property name="keyWord" column="KEY_WORDS"/>
		<property name="webUrl" column="WEB_URL"/>
		<property name="mobileUrl" column="MOBILE_URL"/>
		<property name="picUrl" column="PIC_URL"/>
		<property name="longitude" column="LONGITUDE"/>
		<property name="latitude" column="LATITUDE"/>
		<property name="pubDate" column="PUB_DATE"/>
		<property name="pageView" column="PAGE_VIEW"/>
		<property name="title" column="TITLE"/>
		<many-to-one name="category" column="CATEGORY"/>
	</class>
</hibernate-mapping>
